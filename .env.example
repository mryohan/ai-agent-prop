GOOGLE_CLOUD_PROJECT_ID=gen-lang-client-0734676219
PORT=80801

# Multi-tenant mode: set to 'true' to support multiple websites with separate property listings
# Each tenant will have properties at: {TENANT_ID}/properties.json in GCS or {COLLECTION}_{TENANT_ID} in Firestore
MULTI_TENANT_MODE=false

PROPERTIES_GCS_BUCKET=your-gcs-bucket-name  # optional: set to gs bucket to load properties
PROPERTIES_GCS_PATH=properties.json
PROPERTIES_POLL_SEC=3600  # number of seconds between property list refreshes
PROPERTIES_STORE=gcs # 'gcs' | 'firestore' | 'local'
PROPERTIES_FIRESTORE_COLLECTION=properties

# Email Configuration
# Option 1: Brevo (recommended - https://www.brevo.com)
EMAIL_PROVIDER=BREVO
BREVO_API_KEY=your-brevo-api-key
EMAIL_PROVIDER_ENDPOINT=https://api.brevo.com/v3/
EMAIL_FROM=your-verified-email@example.com

# Option 2: SendGrid
# EMAIL_PROVIDER=sendgrid
# SENDGRID_API_KEY=your-sendgrid-api-key
# EMAIL_FROM=no-reply@example.com

# Agent Information (for email notifications)
AGENT_NOTIFICATION_EMAIL=agent@example.com
AGENT_NAME=Your Agent Name
AGENT_PHONE=+62 XXX XXXX XXXX

PUBSUB_AUTH_AUDIENCE=https://your-cloud-run-url/a/ai-agent-prop/gcs-notify

# Admin Dashboard
# Generate a secure random key: openssl rand -hex 32
ADMIN_KEY=your-secure-admin-key-here
